{
  "name": "vue-highcanfly",
  "homepage": "https://www.highcanfly.club/",
  "version": "1.1.0",
  "description": "HighCanFly website with Vue3/TailwindCSS",
  "repository": "https://github.com/eltorio/vue-highcanfly",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "wrangler": "npx wrangler pages dev -- yarn serve",
    "build": "vue-cli-service build && gulp licenses",
    "lint": "vue-cli-service lint",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@popperjs/core": "2.11.2",
    "@sanity/client": "^3.0.3",
    "@sanity/image-url": "^1.0.1",
    "@tailwindcss/forms": "^0.4.0",
    "@vue/compiler-sfc": "^3.2.29",
    "chart.js": "^2.9.4",
    "core-js": "^3.21.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "qrcode.vue": "^3.3.3",
    "sanity-blocks-vue-component": "^1.0.1",
    "vue": "^3.2.27",
    "vue-router": "^4.0.12",
    "vue3-openlayers": "^0.1.57",
    "walkjs": "^3.2.4",
    "wrangler": "^0.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@vue/cli-plugin-babel": "^5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.2",
    "@vue/cli-service": "^5.0.0-rc.2",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-plugin-vue": "^8.4.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.16",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
