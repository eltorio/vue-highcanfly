{
  "name": "vue-highcanfly",
  "sideEffects": [
    "src/assets/styles/index.css"
  ],
  "homepage": "https://www.highcanfly.club/",
  "version": "1.1.0",
  "description": "HighCanFly website with Vue3/TailwindCSS",
  "repository": "https://github.com/eltorio/vue-highcanfly",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "wrangler": "npx wrangler pages dev -- yarn serve",
    "build": "vue-cli-service build && gulp licenses",
    "build-cf": "git fetch --recurse-submodules && vue-cli-service build --report && gulp licenses",
    "build-report": "vue-cli-service build --report",
    "lint": "vue-cli-service lint",
    "get-meteo-icons": "node get-meteo-icons.js",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "~6.0.0",
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.6",
    "@popperjs/core": "2.11.2",
    "@sanity/client": "^3.2.0",
    "@sanity/image-url": "^1.0.1",
    "@vue/compiler-sfc": "^3.2.31",
    "CFDTrackJoiner": "file:./CFDTrackJoiner",
    "chart.js": "^3.7.1",
    "core-js": "^3.21.1",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "qrcode.vue": "^3.3.3",
    "sanity-blocks-vue-component": "^1.0.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vue3-openlayers": "^0.1.63",
    "walkjs": "^3.2.4",
    "crypto-js": "^4.1.1",
    "element-plus":"~2.0.6",
    "vue-instantsearch": "~4.3.3",
    "vue-agile":"^2.0.0",
    "basiclightbox":"^5.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^8.5.0",
    "gitlog": "^4.0.4",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "@tailwindcss/typography":"^0.5.2",
    "@tailwindcss/forms": "^0.5.0",
    "webpack": "~5.70.0",
    "wrangler": "0.0.19",
    "node-fetch": "~2.6.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
