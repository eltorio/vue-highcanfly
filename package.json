{
  "name": "vue-HighCanFly",
  "homepage": "https://demos.creative-tim.com/vue-notus/",
  "version": "1.1.0",
  "description": "HighCanFly website with Vue3/TailwindCSS",
  "repository": "https://github.com/eltorio/vue-highcanfly",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && gulp licenses",
    "lint": "vue-cli-service lint",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run build:tailwind && npm run serve"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.1",
    "@tailwindcss/forms": "0.2.1",
    "@vue/compiler-sfc": "3.2.28",
    "chart.js": "2.9.4",
    "core-js": "3.20.3",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.9",
    "qrcode.vue": "^3.3.3",
    "tailwindcss": "3.0.15",
    "vue": "3.2.27",
    "vue-router": "4.0.12",
    "vue3-openlayers": "^0.1.56"
  },
  "devDependencies": {
    "@babel/core": "7.16.10",
    "@babel/eslint-parser": "7.13.10",
    "@vue/cli-plugin-babel": "5.0.0-rc.2",
    "@vue/cli-plugin-eslint": "5.0.0-rc.2",
    "@vue/cli-service": "5.0.0-rc.2",
    "autoprefixer": "10.2.5",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.7.0",
    "postcss": "8.4.5",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
